.include /etc/pulse/default.pa

# Disable muting applications during audio calls
unload-module module-role-cork

# Enable switching sinks/inputs when they are connected
load-module module-switch-on-connect

# Earthshaker: Use the correct HDMI profile for audio
set-card-profile alsa_card.pci-0000_01_00.1 output:hdmi-stereo-extra1
