#!/bin/sh
# openbox/autostart - Autostarts for Openbox WM

# tint2
tint2 &

# power
xfce4-power-manager &

# neap
neap &

# compton
compton &

# pulseaudio + volumed + pnmixer
( sleep 1s && ~/.bin/pulseaudio-oneshot ) &

# xrandr
( sleep 1s && ~/.bin/xrandr-adjust ) &

# fehbg
( sleep 3s && ~/.fehbg ) &
