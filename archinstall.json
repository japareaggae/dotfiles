{
	"audio": "pipewire",
	"bootloader": "systemd-bootctl",
	"custom-commands": [
		"curl -o /etc/systemd/zram-generator.conf https://raw.githubusercontent.com/japareaggae/dotfiles/main/zzz-others/zram-generator.conf",
		"sed -i 's/myhostname/myhostname mdns_minimal [NOTFOUND=return]/' /etc/nsswitch.conf",
		"systemctl --global enable dbus-broker.service"
	],
	"filesystem": "btrfs",
	"harddrive": {
		"path": "/dev/?????"
	},
	"hostname": "?????",
	"kernels": [
		"linux-zen"
	],
	"keyboard-language": "br-abnt2",
	"mirror-region": "United States",
	"nic": {
		"NetworkManager": true
	},
	"packages": [
		"alacritty",
		"dbus-broker",
		"exa",
		"fd",
		"firefox",
		"git",
		"htop",
		"lostfiles",
		"mpv",
		"neovim",
		"noto-fonts",
		"noto-fonts-emoji",
		"nss-mdns",
		"pacman-contrib",
		"pulsemixer",
		"ranger",
		"reflector",
		"ripgrep",
		"stow",
		"sway",
		"swayidle",
		"swaylock",
		"ttf-dejavu",
		"ttf-hack",
		"ttf-liberation",
		"wl-clipboard",
		"xorg-xwayland",
		"zram-generator",
		"zsh"
	],
	"profile": "minimal",
	"!root-password": "?????",
	"services": [
		"avahi-daemon",
		"dbus-broker",
		"systemd-timesyncd"
	],
	"superusers": {
		"?????": {
			"!password": "?????"
		}
	},
	"sys-encoding": "utf-8",
	"sys-language": "en_US",
	"timezone": "America/Porto_Velho",
	"users": {}
}
