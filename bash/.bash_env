#
# ~/.bash_env
#

# Preferred Applications
export EDITOR=vim
[[ $DISPLAY ]] && export BROWSER=xdg-open

# PATH
export PATH=$HOME/.bin:$PATH

# Arch Build System / asp
export ABSROOT="$HOME/Development/abs"
export ASPROOT="$HOME/Development/asp"

# pass
export PASSWORD_STORE_CLIP_TIME=15

